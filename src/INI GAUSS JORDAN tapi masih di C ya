INFO: NANTI DIGANTI KE JAVA

#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>

void swap(float m[100][100], int i){
    float temp;
    int k;
    for(k = 0; k < 3; k++){
        temp = m[i-1][k];
        m[i-1][k] = m[i][k]; 
        m[i][k] = temp;
    }
}

int main(){
    float m[100][100];
    int h, i, j, k;
    float temp;
    float x;
    for(i = 0; i < 3; i++){
        for(j = 0; j < 3; j++){
            scanf("%f", &x);
            m[i][j] = x;
        }
    }
    float divider;
    for(h = 0; h < 3; h++){
        for(i = 1; i < 3; i++){
            for(j = 0; j < 3; j++){
                if(m[i][j] != 0 && m[i-1][j] == 0){
                    swap(m, i);
                    break;
                }
            }
        }
    }
    for(i = 1; i < 3; i++){
        for(j = 0; j < i; j++){
            divider = m[i][j] / m[j][j];
            for(k = 0; k < 3; k++){
                m[i][k] -= divider*m[i-j][k];
            }
        }
    }
    printf("\n");
    for(i = 0; i < 3; i++){
        for(j = 0; j < 3; j++){
            printf("%0.2f ", m[i][j]);
        }
        printf("\n");
    }
}
